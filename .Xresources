urxvt.scrollBar: false
!urxvt.transparent: true
!urxvt.shading: 20

!urxvt.tintColor: #172A47
!urxvt.inheritPixmap: true
urxvt.depth: 32

urxvt.keysym.Control-Shift-V: perl:clipboard:paste
urxvt.keysym.Control-Shift-C: perl:clipboard:copy

urxvt.iso14755: False

urxvt.perl-ext-common: default,clipboard

!Fonts

!urxvt.font: xft:Ubuntu Mono:pixelsize=17,style=regular
!urxvt.boldFont: xft:Ubuntu Mono:pixelsize=17,style=bold
urxvt.font: xft:Hack-Regular:pixelsize=22, style=regular
urxvt.boldFont: xft:Hack-Bold:pixelsize=22, style=bold
urxvt.letterSpace: -1


Xft.dpi: 130
Xft.antialias: true
!Xft.rbga: none
Xft.hintstyle: hintfull
Xft.hinting: true

*.internalBorder: 13


*.foreground: #dc6f46
!*.foreground: #663047
*.background: [75]#251f39
*.cursorColor:  #dc6f46




